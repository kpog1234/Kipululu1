{
  "name": "ALPHA-MD",
  "description": "Alpha is a multidevice WhatsApp bot built to make your WhatsApp enjoyable..",
  "keywords": [
    "bot",
    "node",
    "baileys",
    "whatsapp"
  ],
  "logo": "https://telegra.ph/file/b148f6ed21fa02ac5325b.jpg",
  "repository": "https://github.com/Keithkeizzah/ALPHA-MD1",
  "succes_url": "/",
  "stack": "container",
  "env": {
    "PREFIX": {
      "description": "Choose your bot's prefix",
      "value": ".",
      "required": true
    },
    "AUTO_READ_STATUS": {
      "description": "Your contact status will be read automatically (type 'yes' to activate or 'no' to deactivate; don't write in capital letters)",
      "value": "yes",
      "required": false
    },
    "AUTO_REPLY": {
      "description": "This sets a greeting message when someone texts you automatically",
      "value": "yes",
      "required": true
    },
    "AUTO_LIKE_STATUS": {
      "description": "Your contact status will be liked automatically (type 'yes' to activate or 'no' to deactivate; don't write in capital letters)",
      "value": "yes",
      "required": true
    },
    "CHAT_BOT": {
      "description": "If turned on, the chatbot will automatically chat in your private messages or groups.",
      "value": "no",
      "required": false
    },
    "AUTO_DOWNLOAD_STATUS": {
      "description": "Your contact status will be downloaded automatically and sent to you (type 'yes' to activate or 'no' to deactivate; don't write in capital letters)",
      "value": "no",
      "required": false
    },
    "PM_PERMIT": {
      "description": "Restrict other users from using your bot in private messages (type 'yes' to activate or 'no' to deactivate; don't write in capital letters)",
      "value": "no",
      "required": true
    },
    "ANTI_VV": {
      "description": "Detect view-once messages",
      "value": "no",
      "required": false
    },
    "ANTILINK": {
      "description": "Detect view-once links",
      "value": "no",
      "required": false
    },
    "BOT_NAME": {
      "description": "Give a name for your bot",
      "value": "ALPHA-MD",
      "required": false
    },
    "BOT_MENU_LINKS": {
      "description": "Add one or many links for your bot menu (e.g., url1,url2,url3)",
      "value": "",
      "required": false
    },
    "PUBLIC_MODE": {
      "description": "Set 'yes' to make your bot public or 'no' to keep it private",
      "value": "yes",
      "required": false
    },
    "HEROKU_API_KEY": {
      "description": "Insert your Heroku API key (required)",
      "value": "",
      "required": true
    },
    "HEROKU_APP_NAME": {
      "description": "Insert your Heroku app name (required)",
      "value": "Kipululu_wa_twitter",
      "required": true
    },
    "SESSION_ID": {
      "description": "Enter your session ID, the code you receive after scanning the QR code",
      "value": "",
      "required": true
    },
    "OWNER_NAME": {
      "description": "your name",
      "value": "Kipululu_wa_twitter",
      "required": true
    },
    "AUTO_READ_MESSAGES": {
      "description": "yes toactivate no to deactivate",
      "value": "yes",
      "required": false  
    },
    "NUMERO_OWNER": {
      "description": "Your phone number (exclude '+' and area code, e.g., '254796299159')",
      "value": "255679630714",
      "required": false
    },
    "WARN_COUNT": {
      "description": "Set the warning limit for warning commands",
      "value": "3",
      "required": false
    },
    "STARTING_BOT_MESSAGE": {
      "description": "Set 'yes' to display a starting bot message or 'no' to disable it",
      "value": "yes",
      "required": true
    },
    "PRESENCE": {
      "description": "Mark your presence: 1 for online, 2 for typing, 3 for recording, or leave blank for real state",
      "value": "typing",
      "required": true
    },
    "ANTI_DELETE_MESSAGE": {
      "description": "Enable anti-delete message (set 'yes' to activate or 'no' to deactivate)",
      "value": "yes",
      "required": false
    },
    "ANTICALL": {
      "description": "Automatically decline incoming calls (set 'on' to activate or 'off' to deactivate)",
      "value": "yes",
      "required": false
    },
    "AUTO_REACTION": {
      "description": "Set 'yes' to enable bot reactions to messages in private chats",
      "value": "no",
      "required": false
    }
  },
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "standard-2x"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql:essential-0"
    }
  ]
}
